{
    "name": "PostgreSQL",
    "id": "postgresql",
    "version": "0.0.1",
    "description": "Installs PostgreSQL using dnf",
    "options": {},
    "customizations": {
        "vscode": {
            "extensions": [
                "ckolkman.vscode-postgres"
            ]
        }
    },
    "postAttachCommand": "psql -h localhost --username=pgadmin --list",
    "mounts": [
        {
            "source": "data",
            "target": "/var/lib/postgresql/data",
            "type": "volume"
        }
    ]
}
