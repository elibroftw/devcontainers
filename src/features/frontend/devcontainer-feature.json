{
    "name": "Frontend",
    "id": "frontend",
    "version": "0.0.9",
    "description": "Install packages needed for frontend development",
    "options": {},
    "customizations": {
        "vscode": {
            "extensions": [
                "astro-build.astro-vscode",
                "NuclleaR.vscode-extension-auto-import",
                "aeschli.vscode-css-formatter",
                "oven.bun-vscode",
                "dbaeumer.vscode-eslint",
                "svelte.svelte-vscode",
                "jasonnutter.search-node-modules"
            ]
        }
    },
    // use .nvmrc specified version if the file exists otherwise default to lts
    "postAttachCommand": "(. \"$HOME/.bashrc\" && nvm use 2&>/dev/null && corepack enable) || true"
}
