{
    "name": "frontend",
    "id": "frontend",
    "version": "0.0.2",
    "description": "Include packages needed for frontend development",
    "options": {},
    "customizations": {
        "vscode": {
			"extensions": [
				"astro-build.astro-vscode",
                "NuclleaR.vscode-extension-auto-import",
                "aeschli.vscode-css-formatter",
                "oven.bun-vscode",
                "dbaeumer.vscode-eslint",
                "svelte.svelte-vscode",
                "jasonnutter.search-node-modules"
			]
		}
    },
    // use .nvmrc specified version if the file exists
    "postStartCommand": "(nvm install && corepack enable) || true"
}
